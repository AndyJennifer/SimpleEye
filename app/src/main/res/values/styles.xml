<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    
    <declare-styleable name="MultipleStateView">
        
        <attr name="empty_view" format="reference"/>
        <attr name="error_view" format="reference"/>
        <attr name="content_view" format="reference"/>
        <attr name="net_error_view" format="reference"/>
        <attr name="loading_view" format="reference"/>
    </declare-styleable>
</resources>
